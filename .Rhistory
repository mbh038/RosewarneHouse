else e <- c(e,1)
}
## Sum digits of any number
digitsum <- function(x){
num <- floor(log10(x))
digsum <- 0
for(n in num:0){
digsum <- digsum + x %/% 10^n
x <- x %% 10^n
}
return(digsum)
}
