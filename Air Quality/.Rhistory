## Build the continued fraction sequence of e
e <- 2
for(i in 2:100){
if(i%%3==0){e <- c(e,2*i/3)}
else e <- c(e,1)
}
## Sum digits of any number
digitsum <- function(x){
num <- floor(log10(x))
digsum <- 0
for(n in num:0){
digsum <- digsum + x %/% 10^n
x <- x %% 10^n
}
return(digsum)
}
## Code that computes the numerators and denominators of convergents
p <- c(e[1],e[1]*e[2]+1)
q <- c(1,e[2])
for(i in 3:100){
p <- c(p,e[i]*p[i-1]+p[i-2])
q <- c(q,e[i]*q[i-1]+q[i-2])
}
digitsum(p[100])
